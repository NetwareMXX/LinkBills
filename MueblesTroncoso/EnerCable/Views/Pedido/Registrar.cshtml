
@{
    ViewBag.Title = "Facturación";
    Layout = "~/Views/Shared/_LayoutMain.cshtml";
}

<h2>Facturación</h2>

@section Scripts {
    @Scripts.Render("~/scripts/Pedido/Operaciones.js")
    @Scripts.Render("~/scripts/Pedido/Registrar.js")


}


<div id="view" class="card">
    <div class="header">


        <ul class="header-dropdown m-r--5">

            <li class="dropdown">
                <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">
                    <i class="material-icons">more_vert</i>
                </a>
                <ul class="dropdown-menu pull-right">
                    <li><a id="aCapturar" href="javascript:void(0);" class=" waves-effect waves-block"><i class="material-icons">screen_rotation</i>Capturar</a></li>
                    <li><a href="javascript:void(0);" class=" waves-effect waves-block"><i class="material-icons">save</i>Guardar</a></li>
                    <li><a href="javascript:void(0);" class=" waves-effect waves-block"><i class="material-icons">cancel</i>Cancelar</a></li>
                    @*<li><a href="javascript:void(0);" class=" waves-effect waves-block"><i class="material-icons">delete</i>Eliminar</a></li>*@
                </ul>
            </li>
        </ul>
    </div>
    <div class="body">

        <div class="row clearfix">
            <div>
                <div class="panel-group" id="accordion_11" role="tablist" aria-multiselectable="true">
                    <div class="panel panel-col-red">
                        <div class="panel-heading" role="tab" id="headingOne_11">
                            <h4 class="panel-title">
                                <a id="aPanel" role="button" data-toggle="collapse" data-parent="#accordion_11" href="#collapseOne_11" aria-expanded="false" aria-controls="collapseOne_11" class="collapsed">
                                    Buscar Facturas
                                </a>

                            </h4>


                        </div>
                        <div id="collapseOne_11" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingOne_11" aria-expanded="false">
                            <div class="panel-body">
                                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                    <form>

                                        <div class="row clearfix">
                                            <div class="col-sm-3">
                                                <label for="ddlTipoBusqueda">Tipo de Busqueda</label>
                                                <div class="form-group">
                                                    <div class="form-line">
                                                        <input type="hidden" value="0" id="hdIdPedido" />
                                                        <select id="ddlTipoBusqueda" class="combo form-control show-tick">
                                                            <option value="1" selected="selected">Fechas</option>
                                                            <option value="2">Folio Pedido</option>
                                                            <option value="3">Cliente</option>
                                                        </select>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        <div id="rowFecha" class="row clearfix opciones">
                                            <div class="col-sm-3">
                                                <label for="txtFechaInicio">Fecha Inicio</label>
                                                <div class="form-group">
                                                    <div class="form-line">
                                                        <input type="text" id="txtFechaInicio" class="datepicker form-control" placeholder="Por Favor Seleccione la Fecha de Inicio...">
                                                    </div>
                                                </div>

                                            </div>
                                            <div class="col-sm-3">

                                                <label for="txtFechaFin">Fecha Fin</label>
                                                <div class="form-group">
                                                    <div class="form-line">
                                                        <input type="text" id="txtFechaFin" class="datepicker form-control" placeholder="Por Favor Seleccione la Fecha Fin...">
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div id="rowFolio" style="display:none" class="row clearfix opciones">
                                            <div class="col-sm-6">
                                                <label for="txtFolioPedido">Folio de Pedido</label>
                                                <div class="form-group">
                                                    <div class="form-line">
                                                        <input type="text" id="txtFolioPedido" class="form-control" placeholder="Por Favor Ingrese el Folio del Pedido...">
                                                    </div>
                                                </div>

                                            </div>

                                        </div>
                                        <div id="rowCliente" style="display:none" class="row clearfix opciones">
                                            <div class="col-sm-6">
                                                <label for="txtCliente">Cliente</label>
                                                <div class="form-group">
                                                    <div class="form-line">
                                                        <input type="text" id="txtCliente" class="form-control" placeholder="Por Favor Ingrese el cliente...">
                                                    </div>
                                                </div>

                                            </div>

                                        </div>


                                        <div class="row clearfix">
                                            <div class="col-md-2">
                                                <button v-on:click="guardar" type="button" class="btn bg-red waves-effect">
                                                    <i class="material-icons">search</i>
                                                    <span>Buscar</span>
                                                </button>
                                            </div>
                                            <div class="col-md-2">
                                                <button v-on:click="cancelar" type="button" class="btn bg-red waves-effect">
                                                    <i class="material-icons">cancel</i>
                                                    <span>Cancelar</span>
                                                </button>
                                            </div>

                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>


            </div>

        </div>

        <div class="row clearfix">
            <div class="col-md-12">
                <div id="miGrid" kendo-grid="miGrid" class="MyGrid" k-rebind="KendoCulture" options="mainGridOptions"></div>


            </div>
        </div>




        <div id="window" class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <div class="row clearfix">
                <div id="tablaDatos" class="col-md-6">


                </div>
                <div class="col-md-4">
                    <div class="row clearfix">
                        <div id="tablaFecha" class="col-md-4">
                        </div>
                    </div>
                    <div class="row clearfix">
                        <div class="col-md-4">

                            <button type="button" id="btnGuardarPermisos" onclick="return guardarPermiso();" class="btn bg-red waves-effect">
                                <i class="material-icons">save</i>
                                <span>Factura_V33</span>
                            </button>
                        </div>
                    </div>

                </div>
            </div>

            <div class="row clearfix">
                <div class="col-md-10">
                    <div id="tablaMenu" class="table-responsive">

                    </div>

                </div>
            </div>


            <div class="row clearfix">
                <div class="col-md-5">

                    <div class='card'>
                        <div class='header bg-red'>
                            <h2>
                                Parámetros
                            </h2>

                        </div>
                        <div class='body'>




                            <div class="row clearfix">
                                <div class="col-md-10">
                                    <label for="txtUso">Uso CFDI</label>
                                    @Html.DropDownList("txtUso", (IEnumerable

                                            <SelectListItem>)ViewBag.Uso, new { @name = "txtUso", @class = "combo form-control show-tick" })
                                    @*<label for="txtUso">Uso CFDI</label>
                                        <div class="form-group">
                                            <div class="form-line">
                                                <input type="text" id="txtUso" class="form-control" placeholder="Por Favor Ingrese el Uso del CFDI...">
                                            </div>
                                        </div>*@

                                </div>
                            </div>
                            <div class="row clearfix">
                                <div class="col-md-10">
                                    <label for="txtTipoDocumento">Tipo de Documento</label>
                                    @Html.DropDownList("txtTipoDocumento", (IEnumerable

                                            <SelectListItem>)ViewBag.Tipo, new {@name= "txtTipoDocumento", @class = "combo form-control show-tick" })
                                    @*<label for="txtTipoDocumento">Tipo de Documento</label>
                                        <div class="form-group">
                                            <div class="form-line">
                                                <input type="text" id="txtTipoDocumento" class="form-control" placeholder="Por Favor Ingrese el Tipo de Documento...">
                                            </div>
                                        </div>*@
                                </div>
                            </div>

                            <div class="row clearfix">
                                <div class="col-md-10">
                                    <label for="txtRelacionado">CFDI Relacionado (Separados por Enter)</label>
                                    <div class="form-group">
                                        <div class="form-line">
                                            @*<input type="text" id="txtRelacionado" class="form-control" placeholder="Por Favor Ingrese el CFDI Relacionado...">*@
                                            <textarea rows="5" class="form-control no-resize" id="txtRelacionado" placeholder="Por Favor Ingrese el CFDI Relacionado..."></textarea>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>

                <div class="col-md-5">
                    <div class='card'>
                        <div class='header bg-red'>
                            <h2>
                                Importes
                            </h2>

                        </div>
                        <div class='body'>

                            <div class="row clearfix">
                                <div class="col-md-10">
                                    <label for="txtSubTotal">SubTotal</label>
                                    <div class="form-group">
                                        <div class="form-line">
                                            <input type="text" id="txtSubTotal" class="form-control" value="$0.00" readonly="readonly" placeholder="0.00">
                                        </div>
                                    </div>

                                </div>
                            </div>
                            <div class="row clearfix">
                                <div class="col-md-10">
                                    <label for="txtIva">IVA</label>
                                    <div class="form-group">
                                        <div class="form-line">
                                            <input type="text" id="txtIva" class="form-control" value="$0.00" readonly="readonly" placeholder="0.00">
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row clearfix">
                                <div class="col-md-10">
                                    <label for="txtTotal">Total</label>
                                    <div class="form-group">
                                        <div class="form-line">
                                            <input type="text" id="txtTotal" class="form-control" value="$0.00" readonly="readonly" placeholder="0.00">
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>
        @*<div class="row clearfix">
                <div class="col-md-2">
                    <button type="button" id="btnGuardarPermisos" onclick="return guardarPermiso();" class="btn bg-red waves-effect">
                        <i class="material-icons">save</i>
                        <span>Guardar</span>
                    </button>
                </div>
            </div>*@
    </div>






</div>




